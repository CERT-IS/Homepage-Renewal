<%= form_for(current_user, url: unlike_user_path(current_user), method: :delete, remote: true, authenticity_token: true, html: {id: "like_form_#{board.id}"}) do |f| %>
	<i class="fa fa-heart" style="color:#72c02c;"></i> <%= board.likes.count %>
	<input type="hidden" name="board_id" value="<%= board.id %>" />
